local TextLabel = Instance.new("TextLabel")
TextLabel.Parent = game.StarterGui.ScreenGui
TextLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
TextLabel.BorderSizePixel = 0
TextLabel.Position = UDim2.new(0.735348523, 0, 0.254589975, 0)
TextLabel.Size = UDim2.new(0, 200, 0, 50)
TextLabel.Font = Enum.Font.GothamBlack
TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.TextSize = 30.000
TextLabel.Active = true
TextLabel.Draggable = true
local function CQBE_fake_script() -- TextLabel.LocalScript 
	local script = Instance.new('LocalScript', TextLabel)

	while wait() do
		TextLabel.Text = game.Stats.Network.ServerStatsItem["Data Ping"]:GetValueString()
	end
end
coroutine.wrap(CQBE_fake_script)()
