game.RunService.Stepped:Connect(function()game.ReplicatedStorage.MainEvent:FireServer("JoinCrew",getgenv().CrewId)game.ReplicatedStorage.MainEvent:FireServer("LeaveCrew")game.Players.LocalPlayer.Backpack:FindFirstChild("[SprayCan]").Parent=game.Players.LocalPlayer.Character;game.Players.LocalPlayer.MainScreenGui.Crew.CrewFrame.Visible=false end)game.Players.LocalPlayer.Character.BodyEffects["K.O"]:Destroy()game.RunService:Set3dRenderingEnabled(false)wait(180)game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame+Vector3.new(0,99,0)game.Players.LocalPlayer.Character.Humanoid.Health=0;for a,b in pairs(game.Players.LocalPlayer.Character:GetChildren())do if b:IsA("BasePart")then b:Destroy()end end
